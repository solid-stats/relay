HOST=127.0.0.1
PORT=8787
TRUST_PROXY=1

RELAY_TARGET_URL=https://sg.zone
RELAY_RATE_LIMIT_PER_MINUTE=600

RELAY_TOKEN_SECRET=replace-with-long-random-secret-at-least-32-characters
RELAY_TOKEN_TTL_DAYS=30
RELAY_ADMIN_RATE_LIMIT_PER_MINUTE=30

# Who can access /admin* (emails from Authelia headers):
RELAY_ADMIN_EMAILS=admin1@example.com,admin2@example.com

# Trusted auth-proxy headers and secret (Nginx -> relay):
TRUSTED_AUTH_EMAIL_HEADER=remote-email
TRUSTED_AUTH_USER_HEADER=remote-user
TRUSTED_AUTH_SECRET_HEADER=x-auth-proxy-secret
TRUSTED_AUTH_SHARED_SECRET=replace-with-long-random-shared-secret-at-least-32
